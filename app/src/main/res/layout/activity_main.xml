<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">


    <RelativeLayout
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="397dp"
            android:layout_height="659dp"
            tools:layout_editor_absoluteY="63dp"
            tools:layout_editor_absoluteX="6dp"
            android:background="#E9E3E3"
            android:id="@+id/relativeLayout">

        <Button
                android:text="T"
                android:layout_width="76dp"
                android:layout_height="69dp"
                android:id="@+id/btn_tube"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="554dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="304dp"
                android:textSize="30sp"
                android:textColor="#FAF6F6"
                android:background="#999696"
                android:layout_marginTop="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="16dp"/>
        <Button
                android:text="I"
                android:layout_width="78dp"
                android:layout_height="69dp" android:id="@+id/btn_instegram" android:layout_alignParentEnd="true"
                android:layout_marginEnd="207dp" android:layout_marginBottom="555dp"
                android:layout_alignParentBottom="true" android:background="#999696" android:textSize="30sp"
                android:textColor="#E0D1D1" android:layout_marginTop="35dp" android:layout_alignParentTop="true"/>
        <Button
                android:text="F"
                android:layout_width="78dp"
                android:layout_height="68dp" android:id="@+id/btn_facebook" android:layout_alignParentEnd="true"
                android:layout_marginEnd="108dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="557dp" android:background="#999696" android:textColor="#FFFDFD"
                android:textSize="30sp" android:layout_marginTop="34dp" android:layout_alignParentTop="true"/>
        <Button
                android:text="T"
                android:layout_width="75dp"
                android:layout_height="69dp" android:id="@+id/btn_tiwtter" android:layout_alignParentEnd="true"
                android:layout_marginEnd="19dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="557dp" android:background="#999696" android:textSize="30sp"
                android:textColor="#F5F1F1" android:layout_marginTop="33dp" android:layout_alignParentTop="true"/>
        <TextView
                android:text="Tube"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView" android:layout_alignParentEnd="true"
                android:layout_marginEnd="326dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="531dp" android:layout_marginTop="109dp"
                android:layout_alignParentTop="true"/>
        <TextView
                android:text="Instegram"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView2" android:layout_alignParentEnd="true"
                android:layout_marginEnd="215dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="532dp" android:layout_marginTop="108dp"
                android:layout_alignParentTop="true"/>
        <TextView
                android:text="FB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView3" android:layout_alignParentEnd="true"
                android:layout_marginEnd="136dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="533dp" android:layout_marginTop="107dp"
                android:layout_alignParentTop="true"/>
        <TextView
                android:text="Twitter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView4" android:layout_alignParentEnd="true"
                android:layout_marginEnd="29dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="535dp" android:layout_marginTop="105dp"
                android:layout_alignParentTop="true"/>
        <Button
                android:text="C"
                android:layout_width="wrap_content"
                android:layout_height="75dp" android:id="@+id/btn_cuob" android:layout_alignParentEnd="true"
                android:layout_marginEnd="294dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="402dp" android:textSize="30sp" android:background="#999696"
                android:textColor="#FDFDFD" android:layout_marginTop="182dp" android:layout_alignParentTop="true"/>
        <TextView
                android:text="Coub"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView5" android:layout_alignParentEnd="true"
                android:layout_marginEnd="323dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="379dp" android:layout_marginTop="261dp"
                android:layout_alignParentTop="true"/>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="400dp"
                android:layout_height="70dp" android:layout_alignParentBottom="true"
                android:layout_marginBottom="65dp" android:layout_alignParentEnd="true" android:layout_marginEnd="-3dp"
                android:layout_alignParentStart="true" android:layout_marginStart="0dp" android:background="#C0B6B6">
            <Button
                    android:id="@+id/btnGoBack"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:text="@string/back" android:textSize="30sp"/>

            <Button
                    android:id="@+id/btnGoForward"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/btnGoBack"
                    android:text="@string/next" android:textSize="30sp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/button9" android:layout_weight="1"
                    android:textSize="30sp" android:background="@drawable/ic_open_in_browser_black_24dp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/button11" android:layout_weight="1"
                    android:background="@drawable/ic_bookmark_border_black_24dp" android:textSize="30sp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/button10" android:layout_weight="1"
                    android:background="@drawable/ic_invert_colors_black_24dp" android:textSize="30sp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/button12" android:layout_weight="1"
                    android:background="@drawable/ic_collections_bookmark_black_24dp" android:textSize="30sp"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="393dp"
                android:layout_height="80dp" android:layout_alignParentEnd="true" android:layout_marginEnd="4dp"
                android:layout_alignParentBottom="true" android:layout_marginBottom="-4dp"
                android:layout_alignParentStart="true" android:layout_marginStart="0dp" android:background="#C9C1C1">
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="69dp" android:id="@+id/button13" android:layout_weight="1"
                    android:background="@drawable/ic_web_asset_black_24dp"
                    android:textAlignment="center"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="66dp" android:id="@+id/button14" android:layout_weight="1"
                    android:background="@drawable/ic_file_download_black_24dp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="80dp" android:id="@+id/button15" android:layout_weight="1"
                    android:background="@drawable/ic_create_new_folder_black_24dp"/>
            <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/button16" android:layout_weight="1"
                    android:textSize="24sp" android:textStyle="bold"
                    android:background="@drawable/ic_more_horiz_black_24dp"/>
        </LinearLayout>
    </RelativeLayout>
    <EditText
            android:layout_width="335dp"
            android:layout_height="39dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/url_address"
            android:hint="Search or enter website name "
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="672dp"
            app:layout_constraintBottom_toBottomOf="@+id/relativeLayout"
            app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="Go"
            android:layout_width="54dp"
            android:layout_height="42dp"
            android:id="@+id/btn_go"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="668dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textSize="14sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>